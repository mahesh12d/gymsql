[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Redis"
mode = "sequential"
author = 48238828

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3.11 -m api.redis_worker"

[agent]
integrations = ["javascript_database:1.0.0", "javascript_log_in_with_replit:1.0.0", "javascript_gemini:1.0.0"]
expertMode = true


[packager.features]
guessImports = false

[gitHubImport]
requiredFiles = [".replit", "package.json", "requirements.txt"]


[build]
run = "npm install && pip install -r requirements.txt"
